{
  "api": {
    "personalSiteApi": {
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "pitchHandler",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "leadHandler",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "healthHandler",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "function": {
    "pitchHandler": {
      "build": true,
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "personalSiteData",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "leadHandler": {
      "build": true,
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "personalSiteData",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "healthHandler": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "hosting": {
    "amplifyhosting": {
      "providerPlugin": "awscloudformation",
      "service": "amplifyhosting",
      "type": "manual"
    }
  },
  "storage": {
    "personalSiteData": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}